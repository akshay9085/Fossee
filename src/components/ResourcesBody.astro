---
import Accordion from './Accordion.astro'
import Dummy from './Dummy.astro'
---

<div class="resources-main">
    <div class="resources-heading">
        <h1>Resources</h1>
    </div>
    <!-- <Accordion/> -->
    <Dummy/>
</div>
<style>

    .resources-main{
        margin: 3vh;
    }
    .resources-heading h1{
        color: rgb(2, 2, 110);
        font-size: 7.5vh;
        padding: 3vh 21%;
        font-weight: 700;
    }
    .resources-heading:hover{
        color: blue;
    }
    @media screen and (max-width: 983px) {
        .resources-heading h1{
            font-size: 4.5vh;
            padding: 3vh 10%;
            margin-top: 10vh;
            text-align: center;
        }
    }
</style>

